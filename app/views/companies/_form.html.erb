<%=form_for(@company) do |f|%>
<div class = 'row'>
  <div class = 'col-xs-6'>
    <dib class = "form-inputs">
      <%= f.input :name%>
      <%= f.input :website%>
      <%= f.input :phone_numbers%>
      <%= f.input :address%>
      <%= f.attachment_field :logo%>
    </div>
  </div>
</div>
<div class = "form-actions">
  <%= f.button method: :update%>
</div>
<%end%>
